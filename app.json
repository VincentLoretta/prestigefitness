{
  "expo": {
    "name": "prestigefitness",
    "slug": "prestigefitness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "prestigefitness",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.prestigefitness",
      "infoPlist": {
        "NSUserNotificationUsageDescription": "We use notifications to remind you to log your meals."
      }
    },
    "android": {
      "package": "com.prestigefitness.android",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "POST_NOTIFICATIONS",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a8a4949c-2063-48ab-9cb5-b62c9d716eca"
      }
    }
  }
}
